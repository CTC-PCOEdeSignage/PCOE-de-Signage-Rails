
<%= semantic_form_for :users, url: {action: :import_users_csv}, html: {multipart: true} do |f| %>
  <%= f.inputs name: "CSV Import" do %>
    <%= f.input :file, as: :file %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, label: "Import", button_html: {data: {disable_with: "Importing"}} %>
  <% end %>
<% end %>